<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Year Levels</title>
    <style>
        /* In-file CSS */
        body {
            font-family: Arial, sans-serif;
        }
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            padding: 8px;
            text-align: left;
            border: 1px solid #ddd;
        }
        th {
            background-color: #f2f2f2;
        }
        button {
            margin: 5px;
            padding: 10px;
        }
    </style>
</head>
<body>
    <h1>Student Year Levels</h1>
    <a href="/student_year_level/create"><button>Create New Student Year Level</button></a>
    <table>
        <thead>
            <tr>
                <th>Student ID</th>
                <th>Level ID</th>
                <th>School Year ID</th>
                <th>Score</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% student_year_levels.forEach(function(sy) { %>
                <tr>
                    <td><%= sy.student_id %></td>
                    <td><%= sy.level_id %></td>
                    <td><%= sy.school_year_id %></td>
                    <td><%= sy.score %></td>
                    <td>
                        <a href="/student_year_level/update/<%= sy.student_id %>/<%= sy.level_id %>/<%= sy.school_year_id %>"><button>Update</button></a>
                        <a href="/student_year_level/delete/<%= sy.student_id %>/<%= sy.level_id %>/<%= sy.school_year_id %>"><button>Delete</button></a>
                    </td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>